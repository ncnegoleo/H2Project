CREATE TABLE IF NOT EXISTS PROFESSOR (MATRICULA VARCHAR(30) NOT NULL, NOME VARCHAR(50) NOT NULL,CONSTRAINT professor_pk_id PRIMARY KEY (MATRICULA))
CREATE TABLE IF NOT EXISTS CURSO (SIGLA VARCHAR(30) NOT NULL, NOME VARCHAR(50) NOT NULL, CONSTRAINT curso_pk_id PRIMARY KEY (SIGLA))
CREATE TABLE IF NOT EXISTS SALA (CODIGO VARCHAR(30) NOT NULL, BLOCO VARCHAR(50) NOT NULL, CONSTRAINT sala_pk_id PRIMARY KEY (CODIGO))
CREATE TABLE IF NOT EXISTS PERIODO (ID_PERIODO VARCHAR(30) NOT NULL, SIGLA_CURSO VARCHAR(30), CONSTRAINT periodo_pk_id PRIMARY KEY (ID_PERIODO), CONSTRAINT curso_fk_periodo FOREIGN KEY (SIGLA_CURSO)REFERENCES CURSO(SIGLA) ON DELETE CASCADE)
CREATE TABLE IF NOT EXISTS DISCIPLINA (SIGLA_DISCIPLINA VARCHAR(30) NOT NULL, PERIODO VARCHAR(30) NOT NULL, NOME VARCHAR(50) NOT NULL, SIGLA_CURSO VARCHAR(30), CARG_HORARIA INT NOT NULL, CONSTRAINT disciplina_pk_id PRIMARY KEY (SIGLA_DISCIPLINA), CONSTRAINT peri_fk_disciplina FOREIGN KEY (PERIODO)REFERENCES PERIODO(ID_PERIODO) ON DELETE CASCADE, CONSTRAINT curso_fk_disciplina FOREIGN KEY (SIGLA_CURSO)REFERENCES CURSO(SIGLA) ON DELETE CASCADE)
CREATE TABLE IF NOT EXISTS TURMA (ID_TURMA VARCHAR(30) NOT NULL, ID_CURSO VARCHAR(30) NOT NULL, ID_PROF VARCHAR(30) NOT NULL, ID_DISC VARCHAR(30) NOT NULL,ID_SALA VARCHAR(30) NOT NULL, ID_PERIODO VARCHAR(30) NOT NULL, CONSTRAINT turma_pk_id PRIMARY KEY (ID_TURMA), CONSTRAINT curso_fk_turma FOREIGN KEY (ID_CURSO) REFERENCES CURSO(SIGLA) ON DELETE CASCADE, CONSTRAINT prof_fk_turma FOREIGN KEY (ID_PROF) REFERENCES PROFESSOR(MATRICULA) ON DELETE CASCADE, CONSTRAINT disc_fk_turma FOREIGN KEY (ID_DISC) REFERENCES DISCIPLINA(SIGLA_DISCIPLINA) ON DELETE CASCADE, CONSTRAINT sala_fk_turma FOREIGN KEY (ID_SALA) REFERENCES SALA(CODIGO) ON DELETE CASCADE, CONSTRAINT peri_fk_turma FOREIGN KEY (ID_PERIODO) REFERENCES PERIODO(ID_PERIODO) ON DELETE CASCADE)
DISCONNECT
